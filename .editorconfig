# editorconfig.org
root = true

[*]
indent_size = 4
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.html]
indent_size = 2

[*.xml]
indent_size = 2

[*.css]
indent_size = 2

[*.scss]
indent_size = 2
